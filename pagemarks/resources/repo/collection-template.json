{
    // The HTML page title used for this collection
    "title": "My Bookmarks",

    // Web page analytics
    "analytics": {
        "plausible": {
            "enabled": false,
            "data_domain": "example.com",   // put your domain name here
            "script_url": "https://plausible.io/js/plausible.js"
