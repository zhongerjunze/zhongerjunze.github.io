            "ga_account": "UA-123456789-1",
            "cookie_consent": true,
            "anonymize_ip": true     // https://support.google.com/analytics/answer/2763052
        }
    },

    // The entries of the 'Pinned' dropdown, so you don't have to type them into the 'Filter' box every time
    "pinned-filters": []
}
